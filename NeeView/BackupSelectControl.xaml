<UserControl x:Class="NeeView.BackupSelectControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NeeView"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="400">
    <Grid>
        <StackPanel>
            <CheckBox x:Name="UserSettingCheckBox" Content="設定" IsEnabled="False"/>
            <CheckBox x:Name="HistoryCheckBox" Content="履歴" IsEnabled="False"/>
            <CheckBox x:Name="BookmarkCheckBox" Content="ブックマーク" IsEnabled="False"/>
            <CheckBox x:Name="PagemarkCheckBox" Content="ページマーク" IsEnabled="False"/>
        </StackPanel>
    </Grid>
</UserControl>
