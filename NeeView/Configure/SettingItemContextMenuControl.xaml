<UserControl x:Class="NeeView.Configure.SettingItemContextMenuControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NeeView.Configure"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid x:Name="Root">
        <DockPanel>
            <local:ContextMenuSettingControl x:Name="ContextMenuSettingControl" ContextMenuSetting="{Binding Setting.Memento.MainWindowModel.ContextMenuSetting}" Margin="0,0,5,5" />
        </DockPanel>
    </Grid>
</UserControl>
