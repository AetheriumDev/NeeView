<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <runtime>
    <gcAllowVeryLargeObjects enabled="true" />
    <ThrowUnobservedTaskExceptions enabled="true" />
  </runtime>
  <appSettings>
    <add key="PackageType" value=".zip" />
    <add key="UseLocalApplicationData" value="False" />
    <add key="LibrariesPath" value="." />
  </appSettings>
</configuration>
